{
  "name": "sequlize-config",
  "version": "0.1.0",
  "description": "sequlize + config",
  "author": "gradonday@gmail.com",
  "private": true,
  "main": "src/server.ts",
  "scripts": {
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "tslint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "dev": "rm -rf dist && NODE_ENV=development npx ts-node src/server.ts",
    "dev:watch": "rm -rf dist && DEBUG=app* NODE_ENV=development nodemon --config nodemon.json",
    "dev:debug": "rm -rf dist && DEBUG=* NODE_ENV=development nodemon --config nodemon-debug.json",
    "start:test": "NODE_ENV=test npx ts-node src/server.ts",
    "build:prod": "rm -rf dist && tsc -p tsconfig.build.json",
    "start:prod": "npm run build:prod && NODE_ENV=production node dist/server",
    "test": "jest",
    "test:watch": "DEBUG=app* NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "DEBUG=* NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "8.0.0",
    "@nestjs/core": "8.0.0",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/platform-express": "8.0.0",
    "@nestjs/sequelize": "^8.0.0",
    "@types/body-parser": "1.19.2",
    "@types/lodash": "4.14.178",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "compression": "1.7.4",
    "dotenv": "14.2.0",
    "express-ctx": "0.1.1",
    "helmet": "5.0.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.2.0",
    "save": "2.4.0",
    "sequelize": "6.13.0",
    "sequelize-typescript": "2.1.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.2.5",
    "@types/compression": "^1.7.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/sequelize": "^4.28.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.3",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.3.2",
    "run-script-webpack-plugin": "^0.0.11",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
